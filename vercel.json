{
  "builds": [
    {
      "src": "PAQSBackend/wsgi.py",
      "use": "@vercel/build-python",
      "config": {
        "command": "sh build_files.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "PAQSBackend/wsgi.py"
    }
  ]
}
